var plWaitEnabled = true; 
function disableWaitPage() {
plWaitEnabled = false;
}
function pleaseWait() {
if (!plWaitEnabled) {
return;
}
if (document.getElementById('header')!=null){
document.getElementById('header').style.display="none";
}
if (document.getElementById('footer')!=null){
document.getElementById('footer').style.display="none";
}
if (document.getElementById('main')!=null){
document.getElementById('main').style.display="none";
}
document.getElementById('wait').style.display="block";
window.scroll(0, 0);
if (typeof pleaseWaitSetDynamic=='function') {
pleaseWaitSetDynamic();
}
window.setTimeout("if ( document.images['PleaseWaitImage'] ) {document.images['PleaseWaitImage'].src = document.images['PleaseWaitImage'].src;}",200);
}
function resetPleaseWait() {
if (navigator.userAgent.indexOf('Opera') != -1) {
if (document.getElementById('wait')!=null){
document.getElementById('wait').style.display="none";
}
if (document.getElementById('header')!=null){
document.getElementById('header').style.display="block";
}
if (document.getElementById('footer')!=null){
document.getElementById('footer').style.display="block";
}
if (document.getElementById('main')!=null){
document.getElementById('main').style.display="block";
}
}
document.getElementById('wait').style.display="block";
}
function overrideWaitMessage(newMessage) {
var containingDiv = document.getElementById('wrapper');
var imageList = [];
if ((containingDiv == null) || (containingDiv.childNodes == null)) {
return true;
}
// Remove all child elements, but maintain the list of images which we will add back in later.
while (containingDiv.childNodes.length > 1) {
if (containingDiv.firstChild.nodeName == 'IMG') {
imageList[imageList.length] = containingDiv.firstChild;
}
containingDiv.removeChild(containingDiv.firstChild);
}
// Add in our overriding message.
var newMsgElement = document.createElement('h1');
var newMsgText = document.createTextNode(newMessage);
newMsgElement.appendChild(newMsgText);
containingDiv.appendChild(newMsgElement);
// Add in all images.
for (var i = 0; i < imageList.length; i++) {
containingDiv.appendChild(imageList[i]);
}
return true;
}

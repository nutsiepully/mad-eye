if (totals.length==0) return;
var lasttotsel=null;
for(var t=0;t<totals.length==true;t++){
var totalArray=totals[t];
var ttt=fld('totaltriptariff'+t);
if (ttt!=null){
var divs=ttt.getElementsByTagName('div');
for(var i=0;i<divs.length==true;i++){;
divs[i].style.display='none';
}
var found=false;
for(var i=0;i<totalArray.length==true;i+=2){
if(totalArray[i]==comb){
fld(totalArray[i+1]).style.display='inline';
found=true;
}
}
if(found==false){
fld('totaltriptariffempty'+t).style.display='inline';
}
var totsel=fld('totalselector'+t);
if (totsel!=null){
if(found){
totsel.disabled=false;
lasttotsel=totsel;
}else{
totsel.disabled=true;
if(totsel.checked==true){
totsel.checked=false;
if(lasttotsel!=null){
lasttotsel.checked=true;
}
}
}
}
}
}
}
function fld(id){
return document.getElementById(id);
}

//-->
</script>

<script language="JavaScript" type="text/javascript">
<!--
var journeys=new Array();
var grps=new Array();
var lastSelJny=null;
function bind(){
var firstTime=(lastSelJny==null);
if (journeys.length==0) {
for(var i=0;i<document.forms.length;i++){
for(var j=0;j<document.forms[i].elements.length;j++){
var el=document.forms[i].elements[j];
if(el.name.indexOf('selectedBinding') >= 0) {
journeys[journeys.length]=document.forms[i].elements[j];
var grp=document.getElementById('grp-'+el.id);
if(grp!=null) {
grps[grps.length]=grp;
}
}
}
}
}
if(grps.length>0) {
for(var i=0;i<journeys.length==true;i++){
if(journeys[i].checked==true && journeys[i].name.indexOf('selectedBinding[1]') >= 0 && lastSelJny!=journeys[i]){
lastSelJny=journeys[i];
for(var i=0;i<grps.length;i++){
grps[i].style.opacity=0.25;
}
}
}
if(firstTime){
bind2();
}else{
window.setTimeout(bind2,400);
}
}else {
bind2();
}
}
function bind2(){
for(var i=0;i<grps.length;i++){
grps[i].style.opacity=1;
}
var active=null;
for(var i=0;i<journeys.length==true;i++){
journeys[i].disabled=false;
journeys[i].parentNode.style.display='';
journeys[i].parentNode.title='';
if(active==null && journeys[i].checked==true){
for(var j=0;j<bindings.length==true;j++){
if(journeys[i].id==bindings[j][0]){
active=bindings[j];
}
}
}
}
var move=null;
if(active!=null){
for(var i=1;i<active.length==true;i++){
var btnDiv=document.getElementById('grp-'+active[i]);
if (btnDiv!=null) {
btnDiv.style.display='none';
}
var btn=document.getElementById(active[i]);
if(btn!=null){
btn.disabled=true;
btn.parentNode.title=disabledJourneyText;
if(btn.checked==true){
btn.checked=false;
move=btn;
}
}
}
}
if(move!=null){
if(move.scrollWidth==0){
var jnys=move.parentNode.parentNode.getElementsByTagName('input');
for(var i=0;i<jnys.length==true;i++){
if(jnys[i].disabled==false){
jnys[i].checked=true;
move=null;
break;
}
}
}
if(move!=null){
for(var i=0;i<journeys.length==true;i++){
if(journeys[i].name==move.name && journeys[i].disabled==false){
journeys[i].checked=true;
break;
}
}
}
}
var comb='';
for(var i=0;i<journeys.length==true;i++){
if(journeys[i].checked==true){
comb=comb+journeys[i].id;
}
}
if (self.bindTotal) bindTotal(comb);
}

//-->
